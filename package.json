{
  "name": "minapp-sdk",
  "version": "2.3.0",
  "main": "./lib/index.js",
  "browser": "./lib/web.js",
  "miniprogram": "lib",
  "description": "爱范儿知晓云平台 JS SDK",
  "files": [
    "/lib"
  ],
  "scripts": {
    "test": "npm run test-core && npm run test-wechat && npm run test-web && npm run test-alipay",
    "test-core": "mocha test/core-test-new/*.test.js",
    "test-wechat": "mocha test/wechat/index.js",
    "test-alipay": "mocha test/alipay/index.js",
    "test-web": "./test/web/test.sh",
    "web-dev-server": "anywhere -p 40034 -h localhost -d test/web-dev-server",
    "dev": "export NODE_ENV=development && webpack --config sdk-file/webpack.config.js --mode=development -w",
    "build": "export NODE_ENV=production && webpack --mode=production --config sdk-file/webpack.config.js",
    "lint": "eslint core/**/*.js sdk-file/src/**/*.js"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/ifanrx/repository.git"
  },
  "keywords": [
    "ifanrx"
  ],
  "author": "ifanrx",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/ifanrx/repository/issues"
  },
  "homepage": "https://github.com/ifanrx/repository#readme",
  "versions": {
    "alipay": "2.3.0",
    "web": "2.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/plugin-transform-object-assign": "^7.2.0",
    "@babel/preset-env": "^7.2.3",
    "anywhere": "^1.5.0",
    "babel-loader": "^8.0.5",
    "chai": "^3.5.0",
    "copy-webpack-plugin": "^4.6.0",
    "eslint": "4.19.1",
    "faker": "^4.1.0",
    "ftp-deploy": "^2.3.1",
    "github-api": "^3.0.0",
    "jsdom": "^15.0.0",
    "json-format": "^1.0.1",
    "mocha": "^3.2.0",
    "mochawesome": "^2.2.0",
    "module-alias": "^2.2.0",
    "node-localstorage": "^1.3.0",
    "proxyquire": "^2.1.0",
    "rewire": "^2.5.2",
    "sinon": "^3.2.0",
    "sinon-chai": "^3.3.0",
    "sinon-stub-promise": "^4.0.0",
    "webpack": "^4.28.3",
    "webpack-cli": "^3.2.1"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "console-log-level": "^1.4.1",
    "core-js": "^3.0.1"
  }
}
