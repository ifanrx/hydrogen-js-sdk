{
  "plugins": ["plugins/overloadHelper"],
  "plantuml": {
    "puml": {
      "create": true,
      "destination": "docs/puml"
    },
    "images": {
      "create": true,
      "destination": "docs/images",
      "defaultFormat": "png"
    }
  },
  "opts": {
    "encoding": "utf8",
    "destination": "./docs/web",
    "recurse": true
  },
  "source": {
    "include": [
      "core",
      "sdk-file/src/web"
    ]
  },
  "recurseDepth": 10,
  "sourceType": "module",
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": ["jsdoc","closure"]
  }
}
